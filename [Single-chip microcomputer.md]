# container1
//beep:

#include <reg52.h>

#difine	led P0
#define uchar unsigned char



sbit beep = P3^6;

void delay_ms(unsigned char i);

//P0 = 0xFE;
//led = 0xFE;

void main()
{
	  while(1)
	  {
	     beep = 0;
		 delay_ms(1);
		 beep = 1;
		 delay_ms(1);
		 }
}

void delay_ms(unsigned char i)
{
      unsigned char count_ms, count_us;

	  for(count_ms = 0; count_ms < i; count_ms ++)
	      for(count_us = 0; count_us < 123; count_us ++);
}

